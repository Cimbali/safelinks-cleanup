{
  "manifest_version": 2,
  "name": "Safelinks Cleanup",
  "description": "Cleans up Outlook’s “safelinks” tracking",
  "version": "2.0",
  "author": "Cimbali",
  "applications": {
      "gecko": {
          "id": "safelinks-cleanup@cimba.li",
          "strict_min_version": "91.0"
      }
  },
  "background": {
      "scripts": [
          "src/background.js"
      ]
  },
  "permissions": [
      "messagesModify",
      "messagesRead",
      "compose"
  ]
}
